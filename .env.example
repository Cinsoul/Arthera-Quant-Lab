# Arthera Quant Lab Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
DATABASE_URL=postgresql://your_username:your_password@localhost:5432/arthera_quant
POSTGRES_DB=arthera_quant
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_secure_password

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================
# Generate a secure random string for JWT token signing
# You can use: openssl rand -hex 32
SECRET_KEY=your-super-secret-jwt-key-here-minimum-32-characters

# Admin token for settings API access
SETTINGS_ADMIN_TOKEN=your-admin-token-here

# =============================================================================
# DATA PROVIDER API KEYS
# =============================================================================
# Tushare Financial Data API
# Register at: https://tushare.pro/
TUSHARE_TOKEN=your-tushare-api-token-here

# AKShare (Free financial data, no key required)
AKSHARE_TOKEN=

# QuantEngine License (if applicable)
QUANTENGINE_LICENSE=your-quantengine-license-here

# News API Key
# Register at: https://newsapi.org/
NEWS_API_KEY=your-news-api-key-here

# Financial Modeling Prep API Key
# Register at: https://financialmodelingprep.com/
FMP_API_KEY=your-fmp-api-key-here

# Finnhub API Key
# Register at: https://finnhub.io/
FINNHUB_API_KEY=your-finnhub-api-key-here

# Tiingo API Key
# Register at: https://api.tiingo.com/
TIINGO_API_KEY=your-tiingo-api-key-here

# Twelve Data API Key
# Register at: https://twelvedata.com/
TWELVEDATA_API_KEY=your-twelvedata-api-key-here

# =============================================================================
# REDIS CONFIGURATION
# =============================================================================
REDIS_URL=redis://localhost:6379
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================
# Application environment (development, production, testing)
APP_ENV=development

# API Configuration
API_HOST=0.0.0.0
API_PORT=8002

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8002
VITE_API_URL=http://localhost:8002

# Proxy URLs for data services
REACT_APP_TUSHARE_PROXY_URL=http://localhost:8010/api/v1/tushare
VITE_TUSHARE_PROXY_URL=http://localhost:8010/api/v1/tushare

# =============================================================================
# LOGGING CONFIGURATION
# =============================================================================
LOG_LEVEL=INFO
LOG_FORMAT="%(asctime)s - %(name)s - %(levelname)s - %(message)s"

# =============================================================================
# CACHE CONFIGURATION
# =============================================================================
CACHE_TTL_MARKET_DATA=30
CACHE_TTL_HISTORICAL_DATA=3600
CACHE_TTL_STRATEGY_RESULTS=1800

# =============================================================================
# WEBSOCKET CONFIGURATION
# =============================================================================
WS_HEARTBEAT_INTERVAL=30
WS_MAX_CONNECTIONS_PER_CLIENT=5

# =============================================================================
# DEEPSEEK AI CONFIGURATION (if using AI features)
# =============================================================================
DEEPSEEK_API_KEY=your-deepseek-api-key-here
DEEPSEEK_BASE_URL=https://api.deepseek.com

# =============================================================================
# PRODUCTION SECURITY SETTINGS
# =============================================================================
# Set to false in production
DEBUG=false

# CORS origins (comma-separated list)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,http://localhost:3001,http://127.0.0.1:3001

# SSL/TLS Configuration (for production)
SSL_CERT_PATH=/path/to/ssl/cert.pem
SSL_KEY_PATH=/path/to/ssl/key.pem

# =============================================================================
# MONITORING & ANALYTICS (Optional)
# =============================================================================
# Sentry DSN for error tracking
SENTRY_DSN=your-sentry-dsn-here

# Analytics tracking ID
ANALYTICS_ID=your-analytics-id-here