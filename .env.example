# ================================
# Arthera Trading Engine - Environment Variables
# ================================
# Copy this file to .env and fill in your actual values
# NEVER commit your actual .env file to version control!

# ================================
# Data Source API Keys
# ================================
# Tushare Pro Token (Get from: https://tushare.pro/)
TUSHARE_TOKEN=your_tushare_token_here

# Finnhub API (Get from: https://finnhub.io/)
FINNHUB_API_KEY=your_finnhub_api_key_here

# Alpha Vantage API (Get from: https://www.alphavantage.co/)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# ================================
# AI Model API Keys
# ================================
# DeepSeek AI API (Get from: https://platform.deepseek.com/)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# OpenAI API (Optional - Get from: https://platform.openai.com/)
OPENAI_API_KEY=your_openai_api_key_here

# ================================
# Trading Broker APIs (Optional)
# ================================
# Alpaca Trading API (Get from: https://alpaca.markets/)
ALPACA_API_KEY=your_alpaca_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_here

# ================================
# Cryptocurrency Exchange APIs
# ================================
# Binance API (Get from: https://www.binance.com/en/support/faq/how-to-create-api-key)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here
BINANCE_TESTNET=true

# OKX API (Get from: https://www.okx.com/docs-v5/en/)
OKX_API_KEY=your_okx_api_key_here
OKX_SECRET_KEY=your_okx_secret_key_here
OKX_PASSPHRASE=your_okx_passphrase_here
OKX_TESTNET=true

# Coinbase Pro API (Optional - Get from: https://docs.cloud.coinbase.com/)
COINBASE_API_KEY=your_coinbase_api_key_here
COINBASE_SECRET_KEY=your_coinbase_secret_key_here
COINBASE_PASSPHRASE=your_coinbase_passphrase_here
COINBASE_TESTNET=true

# External data service URL (Optional: For custom data providers)
UNIVERSE_SERVICE_URL=
UNIVERSE_API_KEY=

# ================================
# Database Configuration
# ================================
POSTGRES_URL=postgresql://username:password@localhost:5432/trading_engine
REDIS_URL=redis://localhost:6379

# ==================== Service URLs (Internal - Docker will override these) ====================
QUANT_ENGINE_URL=http://localhost:8001
QUANT_LAB_URL=http://localhost:8002
PAPER_TRADING_URL=http://localhost:8003
DASHBOARD_URL=http://localhost:8004
MARKET_DATA_URL=http://localhost:8001
STRATEGY_RUNNER_URL=http://localhost:8002
RISK_ENGINE_URL=http://localhost:8003
PORTFOLIO_SERVICE_URL=http://localhost:8005
ANALYTICS_SERVICE_URL=http://localhost:8005
AI_AGENTS_URL=http://localhost:8006
CRYPTO_CONNECTORS_URL=http://localhost:8007

# ==================== API Configuration ====================
REQUEST_TIMEOUT=30
HEALTH_TIMEOUT=5
IOS_WEBSOCKET_PORT=8005

# ==================== Data Configuration ====================
POOLS_CONFIG_PATH=config/pools.json
UNIVERSE_SEARCH_PATH=/search

# ==================== System Configuration ====================
DEMO_MODE=true
ENABLE_DASHBOARD_FALLBACKS=1

# ================================
# QuantEngine Integration (Optional)
# ================================
# Path to your QuantEngine installation (if you have it)
QUANT_ENGINE_PATH=/path/to/your/QuantEngine
ML_MODEL_PATH=/path/to/your/MLModelTrainingTool

# ==================== Development Settings ====================
# Set to 'production' for production deployment
ENVIRONMENT=development
LOG_LEVEL=INFO

# ================================
# Security Configuration
# ================================
# Generate strong passwords for production!
SECRET_KEY=your-secret-key-change-in-production
POSTGRES_PASSWORD=change_this_password_in_production
ENCRYPTION_KEY=your_32_character_encryption_key_here

# ================================
# Feature Flags
# ================================
DEMO_MODE=true
ENABLE_REAL_TRADING=false
ENABLE_PAPER_TRADING=true
ENABLE_BACKTESTING=true
ENABLE_CRYPTO_TRADING=true
ENABLE_AI_AGENTS=true
ENABLE_MULTI_AGENT_COORDINATION=true

# ================================
# AI Agents Configuration
# ================================
AI_RESEARCH_AGENT_ENABLED=true
AI_STRATEGY_AGENT_ENABLED=true
AI_NEWS_AGENT_ENABLED=true
AI_RISK_AGENT_ENABLED=true
AI_EXECUTION_AGENT_ENABLED=false

# ================================
# Monitoring & Alerting
# ================================
ENABLE_PROMETHEUS_METRICS=true
ENABLE_GRAFANA_DASHBOARDS=true
GRAFANA_ADMIN_PASSWORD=admin123
PROMETHEUS_PORT=9090

# ================================
# Notification Settings
# ================================
# Email notifications (optional)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password

# Telegram notifications (optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_chat_id

# Webhook notifications (optional)
WEBHOOK_URL=your_webhook_url_here